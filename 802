//解答示範802
#include <stdio.h>
#include <stdlib.h>
 
int main() {
    int num;
    while(1){
        printf("(1)教授\n(2)副教授\n(3)助理教授\n(4)都不是\n(5)結束\n");
        printf("請輸入您的職稱代號:");
        scanf("%d",&num);
        if(num==5) break;
        switch(num){
            case 1:
                printf("\n您的職稱是教授\n");
                break;
            case 2:
                printf("\n您的職稱是副教授\n");
                break;
            case 3:
                printf("\n您的職稱是助理教授\n");
                break;
            case 4:
                printf("\n您的職稱沒有在這選項內\n");
                break;
            default:
                printf("\n您的職稱沒有在這選項內\n");
        }
    }
   
    return 0;
}
